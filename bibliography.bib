@Article{Schoonen1998,
  Title                    = {Metacognitive and Language-Specific Knowledge in Native and Foreign Language Reading Comprehension: An Empirical Study Among {D}utch Students in Grades 6, 8 and 10},
  Author                   = {Rob Schoonen and Jan Hulstijn and Bart Bossers},
  Journal                  = {Language Learning},
