@Article{Moerbeek2006,
  author    = {Mirjam Moerbeek},
  title     = {Power and money in cluster randomized trials: when is it worth measuring a covariate?},
  journal   = {Statistics in Medicine},
  year      = {2006},
  volume    = {25},
  number    = {15},
  pages     = {2607-2617},
  abstract  = {The power to detect a treatment effect in cluster randomized trials can be increased by increasing the number of clusters. An alternative is to include covariates into the regression model that relates treatment condition to outcome. In this paper, formulae are derived in order to evaluate both strategies on basis of their costs. It is shown that the strategy that uses covariates is more cost‐efficient in detecting a treatment effect when the costs to measure these covariates are small and the correlation between the covariates and outcome is sufficiently large. The minimum required correlation depends on the cluster size, and the costs to recruit a cluster and to measure the covariate, relative to the costs to recruit a person. Measuring a covariate that varies at the person level only is recommended when cluster sizes are small and the costs to recruit and measure a cluster are large. Measuring a cluster level covariate is recommended when cluster sizes are large and the costs to recruit and measure a cluster are small. An illustrative example shows the use of the formulae in a practical setting.},
  doi       = {10.1002/sim.2297},
  owner     = {jan},
  timestamp = {2020.05.24},
}

