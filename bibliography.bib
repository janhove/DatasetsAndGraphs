@Misc{Schueppert2010,
  Title                    = {Activation of {L1} orthography in spoken word recognition of a closely related {L2}. {E}vidence from {ERP}},

  Author                   = {Anja Sch\"{u}ppert and Johannes C. Ziegler and Kristina Magnusson and Holger Juul and Kenneth Holmqvist and Charlotte Gooskens},
  HowPublished             = {Poster presented at Experimental Approaches to Perception and Production of Language Variation, 11--12 November, Groningen},
  Year                     = {2010},

  Abstract                 = {This paper investigates one of several factors suggested to cause the well-documented asymmetry in mutual intelligibility between spoken Danish and Swedish. Danes have consistently been reported to decode more items in spoken Swedish than vice versa. There is evidence that spoken Danish contains more optional and obligatory reduction processes than spoken Swedish does. Having a rather conservative orthography compared to Swedish, these reductions are often not reflected in the written form. We assume therefore that, if literate speakers of Danish and Swedish can activate their orthographic knowledge as an additional cue during word recognition of the neighbouring language, this enhances word recognition in both groups, but speakers of Danish confronted with spoken Swedish profit to a larger extent than vice versa. To test whether L1 orthography is activated during spoken word recognition of a closely related L2, we present 29 literate speakers of Danish and 27 literate speakers of Swedish with 110 auditory stimuli divided in two conditions in a translation task. The +O condition contains stimuli whose native spelling is consistent with non-native pronunciation and the ÃÂ¢ÃÂÃÂO condition contains stimuli whose native spelling is inconsistent with non-native pronunciation. Importantly, all L2 items are inconsistent with L1 pronunciation, as they differ in exactly one phoneme such as Swedish tolv /tolv/ and Danish tolv /tol/ ÃÂ¢ÃÂÃÂtwelveÃÂ¢ÃÂÃÂ. Results reveal that translations of +O items were more accurate than those of ÃÂ¢ÃÂÃÂO items, and that +O items elicit a negativity that occurred time-locked to the inconsistency and that was largest at centro-posterior electrodes, suggesting that participants activated their native orthography during spoken word recognition of the neighbouring language.},
  Owner                    = {Jan},
  Timestamp                = {2011.08.25}
}

