@Book{Herdina2002,
  Title                    = {A dynamic model of multilingualism: Perspectives of change in psycholinguistics},
  Author                   = {Philip Herdina and Ulrike Jessner},
  Publisher                = {Multilingual Matters},
  Year                     = {2002},

  Address                  = {Clevedon},

  Abstract                 = {This book provides a novel approach to multilingual phenomena as discussed in psycholinguistic research, drawing together (second) language acquisition theory, bilingualism research and dynamic systems theory. Dynamic systems theory, which has been known in other sciences such as physics, mathematics and psychology for some decades, is applied to multilingual systems in order to discuss changees in multilingual proficiency on an individual level and to provide a holistic description of multilingualism. The dynamic interaction between fully developed and developing systems is modelled, and by going beyond the discussion of the contact between two language systems a bridge between the traditionally separated research areas of second language acquisition and bilingualism is created. The model is unfolded step by step, revealing increasing complexities. The various stages are illustrated by numerous graphs which are intended to help the reader understand the concepts introduced. This book should be of interest to researchers in second language acquisition, bilingualism and multilingualism, cognitive linguistics, dynamic systems theory and research methodology, as well as language planners, administrators and other professinoals in the spread of multilingualism. It should also provide a resource text for more advanced students of linguistics, psycholinguistics and applied linguistics.},
  Owner                    = {jan},
