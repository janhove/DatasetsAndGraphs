@Article{DeGroot2014,
  author   = {Adrianus Dingeman {de Groot}},
  title    = {The meaning of `significance' for different types of research},
  journal  = {Acta Psychologica},
  year     = {2014},
  volume   = {148},
  pages    = {188-194},
  note     = {Translated and annotated by {E}ric-{J}an {W}agenmakers, {D}enny {B}orsboom, {J}osine {V}erhagen, {R}ogier {K}ievit, {M}arjan {B}akker, {A}ngelique {C}ramer, {D}ora {M}atzke, {D}on {M}ellenbergh, and {H}an {L.} {J.} {van der Maas}.},
  abstract = {Abstract Adrianus Dingeman de Groot (1914ÃÂ¢ÃÂÃÂ2006) was one of the most influential Dutch psychologists. He became famous for his work ÃÂ¢ÃÂÃÂThought and Choice in ChessÃÂ¢ÃÂÃÂ, but his main contribution was methodological ÃÂ¢ÃÂÃÂ De Groot co-founded the Department of Psychological Methods at the University of Amsterdam (together with R. F. van Naerssen), founded one of the leading testing and assessment companies (CITO), and wrote the monograph ÃÂ¢ÃÂÃÂMethodologyÃÂ¢ÃÂÃÂ that centers on the empirical-scientific cycle: observationÃÂ¢ÃÂÃÂinductionÃÂ¢ÃÂÃÂdeductionÃÂ¢ÃÂÃÂtestingÃÂ¢ÃÂÃÂevaluation. Here we translate one of De Groot's early articles, published in 1956 in the Dutch journal Nederlands Tijdschrift voor de Psychologie en Haar Grensgebieden. This article is more topical now than it was almost 60 years ago. De Groot stresses the difference between exploratory and confirmatory (ÃÂ¢ÃÂÃÂhypothesis testingÃÂ¢ÃÂÃÂ) research and argues that statistical inference is only sensible for the latter: ÃÂ¢ÃÂÃÂOne ÃÂ¢ÃÂÃÂis allowedÃÂ¢ÃÂÃÂ to apply statistical tests in exploratory research, just as long as one realizes that they do not have evidential impactÃÂ¢ÃÂÃÂ. De Groot may have also been one of the first psychologists to argue explicitly for preregistration of experiments and the associated plan of statistical analysis. The appendix provides annotations that connect De Groot's arguments to the current-day debate on transparency and reproducibility in psychological science.},
  doi      = {10.1016/j.actpsy.2014.02.001},
}

