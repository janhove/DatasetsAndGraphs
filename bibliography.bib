@Article{Vanhove2017,
  author    = {Jan Vanhove},
  title     = {The Influence of Standard and Substandard {D}utch on Gender Assignment in Second Language {G}erman},
  journal   = {Language Learning},
  year      = {2017},
  volume    = {67},
  number    = {2},
  pages     = {431-460},
  abstract  = {This study investigated how standard and substandard varieties of first language (L1) Dutch affect grammatical gender assignments to nouns in second language (L2) German. While German distinguishes between masculine, feminine, and neuter gender, the masculine–feminine distinction has nearly disappeared in Standard Dutch. Many substandard Belgian Dutch varieties, however, still mark this distinction, making them more akin to German than Standard Dutch in this respect. Seventy-one Belgian and 104 Netherlandic speakers of Dutch with varying levels of German proficiency assigned gender-marked German articles to German nouns with Dutch cognates; these gender assignments were then compared to the cognates’ gender in the standard and substandard L1 varieties. While the gender assignments of both Belgian and Dutch participants were strongly influenced by the cognates’ Standard Dutch gender, the Belgians’ responses showed, at best, weak traces of the masculine–feminine distinction in substandard Belgian Dutch. Possible reasons for this weak substandard variety influence are discussed.},
