@Article{McGrew2009,
  Title                    = {{CHC} theory and the human cognitive abilities project: Standing on the shoulders of the giants of psychometric intelligence research},
  Author                   = {Kevin S. Mc{G}rew},
  Journal                  = {Intelligence},
  Year                     = {2009},
  Pages                    = {1-10},
