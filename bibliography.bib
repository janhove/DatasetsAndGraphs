@Article{Der2006,
  Title                    = {Age and sex differences in reaction time in adulthood: {R}esults from the {United Kingdom Health and Lifestyle Survey}},
  Author                   = {Geoff Der and Ian J. Deary},
  Journal                  = {Psychology and Aging},
  Year                     = {2006},
