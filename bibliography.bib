@Article{Jones2006b,
  Title                    = {Perceptual organization masquerading as phonological storage: Further support for a perceptual-gestural view of short-term memory},
  Author                   = {Dylan M. Jones and Robert W. Hughes and William J. Macken},
  Journal                  = {Journal of Memory and Language},
