@Article{MacLeod1992,
  Title                    = {The {S}troop task: The ``gold standard'' of attentional measures},
  Author                   = {{Colin M.} Mac{L}eod},
  Journal                  = {Journal of Experimental Psychology: General},
  Year                     = {1992},
  Pages                    = {12-14},
  Volume                   = {121},

  Abstract                 = {In 1992, it would be virtually impossible to find anyone in cognitive psychology who does not have at least a passing acquaintance with the Stroop effect. Indeed, this generalization could probably be extended to all those who have taken a standard introductory psychology course, where the Stroop task is an almost inevitable demonstration. Anyone who has had to name the color of the ink in which an incompatible color word is printed (e.g., to say "blue" in response to the word yellow printed in blue ink) and who has experienced the unavoidable, stumbling interference that occurs will always remember the experience. The Stroop effect, however, is more than a cognitive curiosity; it continues to play a key role in the understanding of attention.},
  Owner                    = {Jan},
  Timestamp                = {2011.09.05}
}

